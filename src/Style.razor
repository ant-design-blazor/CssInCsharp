<HeadContent>
    @foreach (var item in _cache)
    {
        <style id="@item.Key">
            @foreach (var style in item.Value())
            {
                @style.ToString()
            }
        </style>
    }
</HeadContent>

@code
{
    private static Dictionary<string, Func<CSSObject[]>> _cache = new();

    [Parameter]
    public string Id { get; set; } = $"cssincs-{Guid.NewGuid()}";

    [Parameter]
    public Func<CSSObject[]> StyleFn { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        _cache.TryAdd(Id, StyleFn);
    }
}
